<% include partials/header %>

<div class="container">
    <div class="row text-center" >
    <% AllBooks.forEach(function(Books){ %>
        
            <div class="col-md-3 col-sm-6 catalog">
                    <img class= " img-catalog"src="<%= Books.image %>" alt = "<%= Books.name %>">

                    <a href="/AllBooks/<%= Books._id %>" class="btn btn-lg btn-block btn-primary">$ <%= Books.price %></a>  
            </div> 
    <% }); %>
    </div>
</div>
<% include partials/footer %> 
